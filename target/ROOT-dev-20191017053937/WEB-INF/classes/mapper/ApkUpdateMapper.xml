<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tianyi.helmet.server.dao.app.ApkUpdateDao">

	<resultMap id="BaseResultMap" type="com.tianyi.helmet.server.entity.app.ApkUpdate">
		<id column="id" property="id" jdbcType="INTEGER" />
		<result column="createTime" property="createTime" jdbcType="TIMESTAMP" />
		<result column="clientId" property="clientId" jdbcType="VARCHAR" />
		<result column="apkId" property="apkId" jdbcType="INTEGER" />
		<result column="apkFileType" property="apkFileType" jdbcType="VARCHAR" />
		<result column="createUserId" property="createUserId" jdbcType="INTEGER" />
		<result column="updateTime" property="updateTime" jdbcType="TIMESTAMP" />
		<result column="status" property="status" jdbcType="INTEGER" />
	</resultMap>

	<sql id="Base_Column_List">
		id,createTime,clientId,apkId,apkFileType,createUserId,updateTime,status
	</sql>
	<sql id="Base_Column_List_NoId">
		 createTime,clientId,apkId,apkFileType,createUserId,updateTime,status
	</sql>
	<sql id="conditions">
		<where>
			<if test="clientId != null and clientId != ''">
				and clientId = #{clientId}
			</if>
			<if test="apkId != null ">
				and apkId = #{apkId}
			</if>
			<if test="apkFileType != null and apkFileType != ''">
				and apkFileType = #{apkFileType}
			</if>
			<if test="createUserId != null ">
				and createUserId = #{createUserId}
			</if>
			<if test="createTime != null ">
				and createTime = #{createTime,jdbcType=TIMESTAMP}
			</if>
			<if test="createTime1 != null ">
				and createTime &gt;= #{createTime1,jdbcType=TIMESTAMP}
			</if>
			<if test="createTime2 != null ">
				and createTime &lt;= #{createTime2,jdbcType=TIMESTAMP}
			</if>
			<if test="status != null ">
				and status = #{status}
			</if>
		</where>
	</sql>

	<insert id="insert" parameterType="com.tianyi.helmet.server.entity.app.ApkUpdate" useGeneratedKeys="true" keyProperty="id">
		insert into t_apk_update (
			<include refid="Base_Column_List_NoId" />
		)
		values (
			#{createTime},#{clientId},#{apkId},#{apkFileType},#{createUserId},#{updateTime},#{status}
		)
	</insert>

	<select id="listBy" parameterType="map" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from t_apk_update
		<include refid="conditions"/>
		order by createTime desc
		<include refid="core.page"/>
	</select>

	<select id="countBy" parameterType="map" resultType="int">
		 select count(id)
		 from t_apk_update
		<include refid="conditions"/>
	</select>

	<select id="selectById" parameterType="int" resultMap="BaseResultMap">
		select
		 <include refid="Base_Column_List" />
		from t_apk_update
		 where id = #{id}
	</select>

	<update id="uploadById" parameterType="com.tianyi.helmet.server.entity.app.ApkUpdate">
		update t_apk_update
		  set createTime = #{createTime},
			  clientId = #{clientId},
			  apkId = #{apkId},
			  apkFileType = #{apkFileType},
			  createUserId = #{createUserId},
			  updateTime = #{updateTime},
			  status = #{status}
		where id = #{id}
	</update>

	<update id="updateById" parameterType="com.tianyi.helmet.server.entity.app.ApkUpdate">
		update t_apk_update
		set createTime = #{createTime},
		clientId = #{clientId},
		apkId = #{apkId},
		apkFileType = #{apkFileType},
		createUserId = #{createUserId},
		updateTime = #{updateTime},
		status = #{status}
		where id = #{id}
	</update>

	<update id="updateStatusByImeiAndStatus" parameterType="map">
		update t_apk_update
		set status = #{ustatus}
		where clientId = #{clientId} and status = #{status}
	</update>

	<delete id="deleteById" parameterType="int">
		delete from t_apk_update where id = #{id}
	</delete>

</mapper>