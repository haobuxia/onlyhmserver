package com.tianyi.helmet.server.entity.svc;

import com.tianyi.helmet.server.entity.IdEntity;

/**
 * 服务日志-故障处理工单填写情况
 * <p>
 * Created by liuhanc on 2018/4/2.
 */
public class SvcFaultBrief extends IdEntity implements SvcRwhEntity {
    private String rwh;
    private String oprtId;
    private String siteVideo;//拍摄工地
    private String diggerVideo;//拍摄车辆
    private String faultCheckVideo;//拍摄故障
    private String faultRepairVideo;//拍摄故障修复后
    private String jhPic;//机号照片
    private String timeMile;//时间里程
    private String usageStat;//使用情况
    private String fault;//基本信息
    private String faultResove;//基本信息
    private String faultHandle;//基本信息
    private String faultRepair;//基本信息
    private String opinion;//用户意见

    public SvcFaultBrief() {
    }

    public SvcFaultBrief(String rwh, String oprtId) {
        this.rwh = rwh;
        this.oprtId = oprtId;
    }

    public String getOprtId() {
        return oprtId;
    }

    public void setOprtId(String oprtId) {
        this.oprtId = oprtId;
    }

    public String getSiteVideo() {
        return siteVideo;
    }

    public void setSiteVideo(String siteVideo) {
        this.siteVideo = siteVideo;
    }

    public String getDiggerVideo() {
        return diggerVideo;
    }

    public void setDiggerVideo(String diggerVideo) {
        this.diggerVideo = diggerVideo;
    }

    public String getFaultCheckVideo() {
        return faultCheckVideo;
    }

    public void setFaultCheckVideo(String faultCheckVideo) {
        this.faultCheckVideo = faultCheckVideo;
    }

    public String getFaultRepairVideo() {
        return faultRepairVideo;
    }

    public void setFaultRepairVideo(String faultRepairVideo) {
        this.faultRepairVideo = faultRepairVideo;
    }

    public String getJhPic() {
        return jhPic;
    }

    public void setJhPic(String jhPic) {
        this.jhPic = jhPic;
    }

    public String getTimeMile() {
        return timeMile;
    }

    public void setTimeMile(String timeMile) {
        this.timeMile = timeMile;
    }

    public String getUsageStat() {
        return usageStat;
    }

    public void setUsageStat(String usageStat) {
        this.usageStat = usageStat;
    }

    public String getFault() {
        return fault;
    }

    public void setFault(String fault) {
        this.fault = fault;
    }

    public String getFaultResove() {
        return faultResove;
    }

    public void setFaultResove(String faultResove) {
        this.faultResove = faultResove;
    }

    public String getFaultHandle() {
        return faultHandle;
    }

    public void setFaultHandle(String faultHandle) {
        this.faultHandle = faultHandle;
    }

    public String getFaultRepair() {
        return faultRepair;
    }

    public void setFaultRepair(String faultRepair) {
        this.faultRepair = faultRepair;
    }

    public String getOpinion() {
        return opinion;
    }

    public void setOpinion(String opinion) {
        this.opinion = opinion;
    }

    @Override
    public String getRwh() {
        return rwh;
    }

    public void setRwh(String rwh) {
        this.rwh = rwh;
    }
}
