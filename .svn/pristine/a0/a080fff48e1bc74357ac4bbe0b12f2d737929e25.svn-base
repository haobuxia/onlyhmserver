package com.tianyi.helmet.server.entity.log;

/**
 *  用户日志类型枚举
 *
 * Created by liuhanc on 2017/12/13.
 */
public enum UserLogTypeEnum {
    register("register","注册"),
    logon("logon","登录"),
    logout("logout","退出"),
    mobile("mobile","修改手机"),
    password("password","修改密码"),
    edit("edit","修改资料");

    private String val;
    private String name;

    private UserLogTypeEnum(String val, String name){
        this.val = val;
        this.name = name;
    }

    public String getVal() {
        return val;
    }

    public String getName() {
        return name;
    }
}
