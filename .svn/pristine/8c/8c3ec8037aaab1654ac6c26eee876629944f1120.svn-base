package com.tianyi.helmet.server.vo;

/**
 * 两个数据的存储
 * Created by liuhanc on 2017/11/1.
 */
public class DoubleVo<T, R> {
    private T key;
    private R val;

    public DoubleVo() {
    }

    public DoubleVo(T key, R val) {
        this.key = key;
        this.val = val;
    }

    public T getKey() {
        return key;
    }

    public void setKey(T key) {
        this.key = key;
    }

    public R getVal() {
        return val;
    }

    public void setVal(R val) {
        this.val = val;
    }
}
